{
  "name": "ts-err-as-value",
  "version": "0.0.2",
  "description": "Errors as values in typescript",
  "main": "dist/cjs/index.js",
  "keywords": [
    "result",
    "option",
    "rust",
    "kotlin",
    "errors",
    "errors as values",
    "typescript"
  ],
  "module": "./dist/esm/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "publish-npm": "npm publish --access public",
    "preinstall": "npx only-allow pnpm",
    "start": "node --require ts-node/register --require @babel/register src/types-scratch-board.ts",
    "test": "jest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm clean && node esbuild.config.js"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/esm/*.js",
    "dist/cjs/*.js",
    "types/*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./types/index.d.ts"
    },
    "./globals": {
      "import": "./dist/esm/globals.js",
      "require": "./dist/cjs/globals.js",
      "types": "./types/globals.d.ts"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "repository": "https://github.com/bigbizze/ts-error-as-value",
  "readme": "https://github.com/bigbizze/ts-error-as-value#readme",
  "author": "Charles Anderson",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.5",
    "@types/jest": "^29.5.3",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "esbuild": "^0.25.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-null": "^1.0.2",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "latest",
    "typescript": "latest"
  }
}
